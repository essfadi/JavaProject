package customization;

import java.util.ArrayList;
import platform.component.Show;

/**
 *
 * @author user
 */
class BlockedCollection {
    ArrayList<Show> blocked; 
    
    BlockedCollection(){
        this.blocked=new ArrayList<>(); 
    }

    public ArrayList<Show> getBlocked() {
        return blocked;
    }

    public void setBlocked(ArrayList<Show> Blocked) {
        this.blocked = Blocked;
    }
       
    @Override
    public String toString() {

        return "The list of your blocked Shows is: \n\t BlockedCollection{" + "favorites=" + blocked + '}';
    }
    
}
